{
  "name": "@darkobits/fd-name",
  "version": "0.1.2",
  "license": "WTFPL",
  "description": "ESM replacements for __dirname and __filename.",
  "author": "darkobits",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/fd-name"
  },
  "keywords": [
    "__dirname",
    "__filename",
    "cjs",
    "mjs",
    "esm",
    "modules",
    "alternative",
    "replacement"
  ],
  "files": [
    "src"
  ],
  "main": "src/cjs/index.js",
  "module": "src/mjs/index.js",
  "exports": {
    ".": {
      "import": "./src/mjs/index.js",
      "require": "./src/cjs/index.js"
    }
  },
  "scripts": {
    "prepare": "nr prepare",
    "help": "nr --scripts"
  },
  "dependencies": {
    "callsites": "3.1.0"
  },
  "devDependencies": {
    "@darkobits/ts": "^0.8.4",
    "@types/fs-extra": "^9.0.12",
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "tempy": "^1.0.1"
  }
}
